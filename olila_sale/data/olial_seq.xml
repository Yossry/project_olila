<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	    <record id="seq_res_partner_olila" model="ir.sequence">
	        <field name="name">Olila Code</field>
	        <field name="code">res.partner</field>
	        <field name="prefix">OLILA/</field>
	        <field name="padding">5</field>
	        <field name="company_id" eval="False" />
	    </record>
	    
	    <record id="seq_cost_estimation_olila" model="ir.sequence">
	        <field name="name">Cost Estimation Code</field>
	        <field name="code">cost.estimation</field>
	        <field name="prefix">COST/</field>
	        <field name="padding">5</field>
	        <field name="company_id" eval="False"/>
	    </record>

	    <record id="seq_request_for_quote" model="ir.sequence">
	        <field name="name">Request For Quote</field>
	        <field name="code">request.for.quote</field>
	        <field name="prefix">RFQ/</field>
	        <field name="padding">5</field>
	        <field name="company_id" eval="False" />
	    </record>
	</data>
</odoo>
